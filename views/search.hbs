<div class="container-fluid">

    <div class="row justify-content-center">
        <h1 class='text-center'>Search Results</h1>
    </div>

    <div class="row justify-content-center">
        <ul class="user_list">
            {{#each searchUserList}}
            <li>
                <a href="/dashboard/profile/{{this._id}}">{{this.email}}</a>
            </li>
            {{else}}
            <p>No User found</p>
            {{/each}}
        </ul>
    </div>
</div>